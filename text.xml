<root
        >
    <objects>
        <object name="tmnxSubAcctPlcyRadStatsEntry">
            <description>Each row entry represents a collection of statistics for a specific RADIUS server in a subscriber accounting policy in the system.
                Entries cannot be created and deleted via SNMP SET operations.</description>
            <indexes>
                <index name="tmnxSubAcctPlcyRadServPlcyName">
                    <oid value="1.3.6.1.4.1.6527.3.1.2.33.1.28.1.1"/>
                    <syntax syntax="UNKNOWN"/>
                    <access access="UNKNOWN"/>
                    <values>
                        <value access="12"/>
                        <value access="12"/>
                    </values>
                </index>
                <index name="tmnxSubAcctPlcyRadServIndex">
                    <oid value="1.3.6.1.4.1.6527.3.1.2.33.1.28.1.2"/>
                    <syntax syntax="UNKNOWN"/>
                    <access access="UNKNOWN"/>
                    <values>
                        <value access="12"/>
                        <value access="12"/>
                    </values>
                </index>
            </indexes>
            <indicators>
                <indicator name="tmnxSubAcctPlcyRadTxRequests">
                    <oid>1.3.6.1.4.1.6527.3.1.2.33.1.30.1.1</oid>
                    <access type="read-only"/>
                    <units type="null"/>
                    <snmpSyntax type="Counter32"/>
                    <description>The value of tmnxSubAcctPlcyRadTxRequests indicates the number of accounting requests transmitted for this server.</description>
                </indicator>
                <indicator name="tmnxSubAcctPlcyRadRxResponses">
                    <oid>1.3.6.1.4.1.6527.3.1.2.33.1.30.1.2</oid>
                    <access type="read-only"/>
                    <units type="null"/>
                    <snmpSyntax type="Counter32"/>
                    <description>The value of tmnxSubAcctPlcyRadRxResponses indicates the number of accounting responses received for this server.</description>
                </indicator>
                <indicator name="tmnxSubAcctPlcyRadReqTimeouts">
                    <oid>1.3.6.1.4.1.6527.3.1.2.33.1.30.1.3</oid>
                    <access type="read-only"/>
                    <units type="null"/>
                    <snmpSyntax type="Counter32"/>
                    <description>The value of tmnxSubAcctPlcyRadReqTimeouts indicates the number of accounting requests which have timed out for this server.</description>
                </indicator>
                <indicator name="tmnxSubAcctPlcyRadReqSendFail">
                    <oid>1.3.6.1.4.1.6527.3.1.2.33.1.30.1.4</oid>
                    <access type="read-only"/>
                    <units type="null"/>
                    <snmpSyntax type="Counter32"/>
                    <description>The value of tmnxSubAcctPlcyRadReqSendFail indicates the number of accounting requests failed because the packet could not be
                        sent out.</description>
                </indicator>
            </indicators>
            <otherOids/>
        </object>
    </objects>
</root>